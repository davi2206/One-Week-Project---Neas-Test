<Window x:Class="Business_Manager_UI.AddOrRemoveSalesman"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Business_Manager_UI"
        mc:Ignorable="d"
        Title="Add or Remove Salesman" Height="300" Width="350" Loaded="Window_Loaded">
    <Grid>
        <StackPanel x:Name="Menubar" Orientation="Horizontal" Height="35" VerticalAlignment="Top" Margin="5">
            <ComboBox x:Name="Salesmen" Text="Tilgængelige sælgere" Width="300">
                <ComboBoxItem Name="cbi1">Item1</ComboBoxItem>
                <ComboBoxItem Name="cbi2">Item2</ComboBoxItem>
                <ComboBoxItem Name="cbi3">Item3</ComboBoxItem>
            </ComboBox>
        </StackPanel>
    </Grid>
</Window>
